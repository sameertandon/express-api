{
  "name": "express-api",
  "version": "1.0.0-SNAPSHOT",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "local-start": "npm run build && npm run tsc-example-app",
    "build": "tsc --resolveJsonModule",
    "tsc-example-app": "dyson examples/app/data/mock 9080 | CONFIG_PROFILE=localhost CONFIG_LABEL=master NODE_ENV=localhost LOG_LEVEL=debug node ./examples/app/server.js",
    "lint": "tslint --fix example/**/*.ts src/**/*.ts test/**/*.ts index.ts ",
    "precommit": "npm dedupe && npm run lint && npm run test",
    "test": "npm run build && CONFIG_PROFILE=test CONFIG_LABEL=master NODE_ENV=test nyc mocha"
  },
  "keywords": [
    "openapi3",
    "router"
  ],
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com"
  },
  "dependencies": {
    "ajv": "^6.10.0",
    "lodash": "4.17.15"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-babel": "2.1.1",
    "@istanbuljs/nyc-config-typescript": "0.1.3",
    "@types/express": "4.17.1",
    "@types/lodash": "4.14.138",
    "@types/mocha": "5.2.7",
    "@types/node": "12.7.5",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "compression": "1.7.4",
    "cookie-parser": "1.4.4",
    "dyson": "2.0.5",
    "eslint": "5.4.0",
    "express": "4.17.1",
    "helmet": "3.21.0",
    "husky": "^1.0.0-rc.15",
    "istanbul": "^1.1.0-alpha.1",
    "loopback": "3.26.0",
    "mocha": "^5.2.0",
    "mocha-each": "^1.1.0",
    "mocha-junit-reporter": "^1.13.0",
    "nodemon": "^1.18.10",
    "nyc": "14.1.1",
    "semver": "^5.5.1",
    "source-map-support": "0.5.13",
    "ts-node": "8.3.0",
    "tslint": "5.20.0",
    "typescript": "3.6.3"
  }
